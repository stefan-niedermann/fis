{
  "name": "frontend",
  "version": "0.0.0",
  "author": "Niedermann IT <info@niedermann.it> (https://www.niedermann.it)",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test --silent",
    "lint": "ng lint",
    "e2e:mock": "ng e2e --headless --watch=false",
    "e2e:prod": "ng e2e --headless --watch=false --configuration=production",
    "postinstall": "npm run gen",
    "gen": "npx @openapitools/openapi-generator-cli generate -i ../api.yml -g typescript-angular -o src/app/gen --additional-properties=supportsES6=true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.6",
    "@angular/common": "~13.2.6",
    "@angular/compiler": "~13.2.5",
    "@angular/core": "~13.2.3",
    "@angular/forms": "~13.2.6",
    "@angular/router": "~13.2.6",
    "@angular/platform-browser": "~13.2.6",
    "@angular/platform-browser-dynamic": "~13.2.6",
    "rxjs": "~7.5.5",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-builders/jest": "^13.0.3",
    "@angular-devkit/build-angular": "~13.2.6",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "~13.2.6",
    "@angular/compiler-cli": "~13.2.6",
    "@cypress/schematic": "^1.6.0",
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.15.0",
    "cypress": "9.5.1",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.0",
    "ng-mocks": "^13.1.0",
    "ts-node": "~10.7.0",
    "typescript": "~4.5.5"
  }
}
